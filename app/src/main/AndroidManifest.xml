<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.conways.opengl">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".TriangleActivity"
            android:label="三角形"/>
        <activity
            android:name=".PointActivity"
            android:label="点">
        </activity>
        <activity
            android:name=".LineActivity"
            android:label="线">
        </activity>
        <activity
            android:name=".RectangularActivity"
            android:label="矩形">
        </activity>
        <activity android:name=".CubeActivity">
        </activity>
        <activity android:name=".STLActivity">
        </activity>
        <activity android:name=".stl.MainActivity">
        </activity>
    </application>

</manifest>